 StyleBox["R",
  FontSize->36,
  FontSlant->"Italic"],
 StyleBox[" = ",
  FontSize->36],
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", 
