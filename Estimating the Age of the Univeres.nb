      RowBox[{"(", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", "=", "1"}], "n"], 
        SubscriptBox["X", "i"]}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", "=", "1"}], "n"], 
        SubscriptBox["y", "i"]}], ")"}]}]}], 
    RowBox[{
     SqrtBox[
      RowBox[{
       RowBox[{"(", 
